{
  "name": "firescript",
  "version": "0.3.5",
  "description": "Firescript transpiler",
  "main": "src/app.js",
  "bin": {
    "fire": "./bin/fire"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:integration && npm run test:syntax",
