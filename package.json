{
  "name": "firescript",
  "version": "0.5.0",
  "description": "Firescript transpiler",
  "main": "src/app.js",
  "bin": {
    "fire": "./bin/fire",
    "firetest": "./bin/firetest"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:integration && npm run test:syntax && npm run test:transform",
    "test:unit": "mocha --reporter lagoon-reporter tests/unit/\\*\\*/\\*.spec.js",
    "test:integration": "mocha --reporter lagoon-reporter tests/integration/\\*\\*/\\*.spec.js",
    "test:syntax": "mocha --reporter lagoon-reporter tests/syntax/\\*\\*/\\*.spec.js",
    "test:transform": "mocha --reporter lagoon-reporter tests/transform/\\*\\*/\\*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Andifeind/firescript.git"
  },
  "keywords": [
    "fire",
    "fs",
    "firescript"
  ],
  "author": "Andi Heinkelein <andifeind@noname-media.com>",
  "license": "MIT",
  "devDependencies": {
    "@noname-media/eslint-config-standard": "1.1.0",
    "fireio": "^0.1.0",
    "inspect.js": "^1.11.8",
    "lagoon-reporter": "^0.5.0",
    "mocha": "5.2.0",
    "shell-inspect": "1.2.0",
    "sinon": "^6.1.5"
  },
  "dependencies": {
    "args": "^5.0.0",
    "colorfy": "^2.0.4",
    "eslint-plugin-node": "^7.0.1",
    "esprima": "4.0.1",
    "superarr": "0.1.0",
    "superconf": "^1.2.0",
    "superfs": "^0.12.2",
    "superimport": "^1.4.0",
    "superreg": "^0.2.1",
    "supershit": "^0.5.5"
  }
}
